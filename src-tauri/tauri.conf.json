{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1421"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "subtle",
  "mainBinaryName": "subtle",
  "version": "0.5.3-a0",
  "identifier": "com.haha.subtle",
  "plugins": {},
  "app": {
    "security": {
      "assetProtocol": {
        "scope": [
          "**"
        ],
        "enable": true
      },
      "csp": "default-src 'self' data:; script-src 'self' 'unsafe-inline'; style-src 'self' http: 'unsafe-inline'; img-src 'self' http: data:; media-src 'self' http: asset: https://asset.localhost; connect-src ipc: http://ipc.localhost"
    },
    "windows": [
      {
        "label": "main",
        "title": "subtle",
        "width": 1000,
        "height": 800,
        "visible": true
      }
    ]
  }
}